{
  "name": "mr-pikipiki-trading",
  "version": "1.0.0",
  "description": "MR PIKIPIKI TRADING Management System",
  "main": "server/index.js",
  "type": "module",
  "scripts": {
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "server": "nodemon server/index.js",
    "client": "cd client && npm run dev",
    "build": "cd client && npm install && npm run build",
    "vercel-build": "cd client && npm ci && npm run build",
    "start": "node server/index.js",
    "postinstall": "cd client && npm install",
    "db:init": "node server/database/init-db.js",
    "db:seed": "node server/database/seed.js",
    "db:setup": "npm run db:init && npm run db:seed"
  },
  "keywords": [
    "motorcycle",
    "trading",
    "management"
  ],
  "author": "MR PIKIPIKI TRADING",
  "license": "ISC",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "exceljs": "^4.4.0",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.2",
    "multer": "^1.4.5-lts.1",
    "pdfkit": "^0.14.0",
    "pg": "^8.16.3"
  },
  "devDependencies": {
    "concurrently": "^8.2.2",
    "nodemon": "^3.0.2"
  }
}
