{
  "buildCommand": "cd client && npm install --include=dev && npm run build",
  "outputDirectory": "client/dist",
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/server/vercel.js"
    },
    {
      "source": "/((?!api/)(?!.*\\.(js|mjs|css|ico|png|jpg|jpeg|gif|svg|woff|woff2|ttf|eot|json|pdf|map|webp|avif)$).*)",
      "destination": "/index.html"
    }
  ]
}
